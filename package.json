{
  "name": "@nahmii/sdk",
  "version": "0.0.1",
  "description": "The `@nahmii/sdk` package is a SDK to interact more easily with Nahmii 2.0.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.ts",
    "dist/**/*.json"
  ],
  "scripts": {
    "prepublish": "tsc -p ./tsconfig.build.json",
    "build": "tsc -p ./tsconfig.build.json",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.ts'",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "check-formatting": "prettier --write --list-different './src/**/*.tsx'",
    "prettier": "prettier --write \"src/**/*.{ts,,tsx,js,json}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nahmii/nahmii-2-sdk.git"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "blockchain",
    "crypto",
    "defi",
    "ethereum",
    "nahmii",
    "smart contracts",
    "SDK"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nahmii/nahmii-2-sdk/issues"
  },
  "homepage": "https://github.com/nahmii/nahmii-2-sdk#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsdoc": "^37.2.2",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-unicorn": "^39.0.0",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "ethers": "^5.5.2",
    "lodash": "^4.17.21",
    "rlp": "^2.2.7"
  }
}
